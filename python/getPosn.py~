#
# getPosns.py
#

# Python wrapper for Jay Anderson's img2xym fortran routine. Also does
# some filename carpentry.

class PositionSet(object, pathImg='', doTest=True, Verbose=True):

    def __init__(self):

        self.pathImg=pathImg[:]


        if doTest:
            self.setupTest()

        

    def setupTest(self):

        """Sets some variables for testing"""

        if self.Verbose:
            print("PositionSet.setupTest INFO: setting test conditoins")
        
        self.pathImg='j8q623yvq_flc.fits'
